{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "language": "en",
    "words": [
        "eigenstates"
    ],
    "ignoreWords": [
        "ndarray",
        "rustfft",
        "int_roundings",
        "UPLO",
        "eigh",
        "nrows",
        "serde",
        "Qobj",
        "qutip",
        "cdylib",
        "pyclass",
        "pyfunction",
        "pymodule",
        "inplace",
        "ntraj",
        "Ctarget",
        "maturin",
        "pytest",
        "rustc"
    ],
    "flagWords": [
        "hte",
        "teh"
    ],
    "allowCompoundWords": true,
    "maxNumberOfProblems": 1000,
    "ignorePaths": [
        "*.snap",
        "node_modules",
        "__snapshots__",
        "package-lock.json",
        "coverage/**",
        "dist/**",
        "dist.*/**",
        "temp/**",
        "CHANGELOG.md",
        "package.json",
        ".cspell.json",
        "cspell.json",
        ".vscode/**",
        ".devcontainer/**",
        ".github/**",
        ".pytest_cache/**",
        ".ruff_cache/**",
        ".venv/**",
        ".vscode/**",
        "build/**",
        "docs/**",
        "*.egg-info/**",
        "target/**"
    ],
    "noConfigSearch": true
}